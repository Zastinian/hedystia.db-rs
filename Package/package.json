{
  "name": "hedystiadb",
  "version": "1.7.0",
  "description": "A database made by the company hedystia developed in Rust, easy to use",
  "main": "index.js",
  "types": "index.d.ts",
  "keywords": [
    "Database",
    "Hedystia",
    "Discord",
    "node",
    "db",
    "discord.js",
    "typescript",
    "rust"
  ],
  "scripts": {
    "build": "cargo-cp-artifact -nc index.node -- cargo build --message-format=json-render-diagnostics",
    "build-debug": "npm run build --",
    "build-release": "npm run build -- --release",
    "install": "npm run build-release",
    "test": "cargo test"
  },
  "author": "contact@hedystia.com",
  "license": "ISC",
  "devDependencies": {
    "@types/bun": "latest",
    "cargo-cp-artifact": "^0.1",
    "typescript": "^5.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zastinian/hedystia.db-rs.git"
  },
  "bugs": {
    "url": "https://github.com/Zastinian/hedystia.db-rs/issues"
  },
  "homepage": "https://docs.hedystia.com/dbrs/start",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}